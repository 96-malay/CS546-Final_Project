<div>

<div class="main">
<div class="container">
  {{!-- <h3>{{error}}</h3> --}}
  {{#if trainers}}
  {{#each trainers}} 
  
<div onclick="location.href='/trainers/trainer/{{this._id}}';" class="card" style="width:600px"></div>
    <div class="card-body bg-dark text-white">
        <a href='/trainers/trainer/{{this._id}}'>
